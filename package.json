{
  "name": "@whi/magicauth-sdk",
  "version": "0.2.0",
  "description": "The Javascript standard development kit for MagicAuth services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "API_CONTRACT.md",
    "AUTHENTICATION_FLOW.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@whi/http": "^0.2.1",
    "@whi/stdlog": "^0.3.0",
    "ipaddr.js": "^2.2.0",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^4.0.13",
    "eslint": "^9.39.1",
    "knex": "^3.1.0",
    "prettier": "^3.6.2",
    "sqlite3": "^5.1.7",
    "superstruct": "^0.8.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "author": "Matthew Brisebois",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/magicauth-io/legacy-magicauth-sdk"
  }
}
